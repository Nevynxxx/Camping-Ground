# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
#Ignore Vim Backup Files
*~

# Ignore generated docs, as they can be re-generated
/doc/*
